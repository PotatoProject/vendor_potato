<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <string name="lawnchair_overlay">LawnConf</string>

    <!-- Whether or not blur is enabled per default -->
    <bool name="config_default_enable_blur">true</bool>

    <!--
        Which color resolver to use per default; Possible values:
            ch.deletescape.lawnchair.colors.SystemAccentResolver
            ch.deletescape.lawnchair.colors.PixelAccentResolver
            ch.deletescape.lawnchair.colors.RGBColorResolver|R|G|B
            ch.deletescape.lawnchair.colors.WallpaperMainColorResolver
            ch.deletescape.lawnchair.colors.WallpaperSecondaryColorResolver
            ch.deletescape.lawnchair.colors.WallpaperTertiaryColorResolver
    -->
    <string name="config_default_color_resolver" translatable="false">ch.deletescape.lawnchair.colors.SystemAccentResolver</string>

</resources>
